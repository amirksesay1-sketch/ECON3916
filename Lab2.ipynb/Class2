      "cell_type": "code",
      "source": [
        "import pandas as pd\n",
        "import matplotlib.pyplot as plt\n",
        "\n",
        "# Ensure the index is datetime (safe even if it's already DatetimeIndex)\n",
        "df = df.copy()\n",
        "df.index = pd.to_datetime(df.index)\n",
        "\n",
        "# Plot\n",
        "plt.figure(figsize=(14, 7))\n",
        "\n",
        "plt.plot(\n",
        "    df.index,\n",
        "    df[\"Nominal_Wage\"],\n",
        "    linestyle=\"--\",\n",
        "    color=\"red\",\n",
        "    linewidth=2,\n",
        "    label=\"Nominal Wage\"\n",
        ")\n",
        "\n",
        "plt.plot(\n",
        "    df.index,\n",
        "    df[\"Real_Wage\"],\n",
        "    linestyle=\"-\",\n",
        "    color=\"blue\",\n",
        "    linewidth=3,\n",
        "    label=\"Real Wage\"\n",
        ")\n",
        "\n",
        "plt.title(\"The Illusion of Growth (1964â€“Present)\", fontsize=16, fontweight=\"bold\")\n",
        "plt.xlabel(\"Year\")\n",
        "plt.ylabel(\"Wage Level\")\n",
        "\n",
        "# Annotate the 2020 spike in Real Wages (robust)\n",
        "df_2020 = df.loc[\"2020\"]\n",
        "pandemic_date = df_2020[\"Real_Wage\"].idxmax()           # Timestamp label of max in 2020\n",
        "pandemic_value = df.loc[pandemic_date, \"Real_Wage\"]     # Real_Wage value at that timestamp\n",
        "\n",
        "plt.annotate(\n",
        "    \"Pandemic Composition Effect\",\n",
        "    xy=(pandemic_date, pandemic_value),\n",
        "    xytext=(pandemic_date, pandemic_value * 0.85),\n",
        "    arrowprops=dict(arrowstyle=\"->\", linewidth=1.5),\n",
        "    fontsize=12,\n",
        "    ha=\"center\"\n",
        ")\n",
        "\n",
        "plt.legend()\n",
        "plt.tight_layout()\n",
        "plt.show()\n"
      ],
