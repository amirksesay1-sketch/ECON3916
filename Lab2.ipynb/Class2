    {
      "cell_type": "code",
      "source": [
        "import pandas as pd\n",
        "import matplotlib.pyplot as plt\n",
        "\n",
        "# 1) Fetch ECI wages (Employment Cost Index) using your existing fred object\n",
        "eci = fred.get_series(\"ECIWAG\")\n",
        "eci = pd.Series(eci, name=\"ECIWAG\")\n",
        "\n",
        "# --- Clean + ensure datetime indexes (robust) ---\n",
        "wages = pd.Series(wages, name=\"wages\").copy()\n",
        "wages.index = pd.to_datetime(wages.index)\n",
        "eci.index = pd.to_datetime(eci.index)\n",
        "\n",
        "wages = wages.sort_index()\n",
        "eci = eci.sort_index()\n",
        "\n",
        "# 2) Keep dates from 2015-01-01 onwards\n",
        "start = pd.Timestamp(\"2015-01-01\")\n",
        "wages = wages.loc[wages.index >= start]\n",
        "eci = eci.loc[eci.index >= start]\n",
        "\n",
        "# Optional but recommended: ECI is typically quarterly; make wages comparable by resampling to quarterly mean\n",
        "wages_q = wages.resample(\"Q\").mean()\n",
        "eci_q = eci.resample(\"Q\").last()\n",
        "\n",
        "# Align on common dates\n",
        "aligned = pd.concat([wages_q, eci_q], axis=1).dropna()\n",
        "wages_q = aligned[\"wages\"]\n",
        "eci_q = aligned[\"ECIWAG\"]\n",
        "\n",
        "# 3) Rebase both to 100 (first value = 100)\n",
        "def rebase_to_100(s: pd.Series) -> pd.Series:\n",
        "    s = s.astype(float)\n",
        "    return (s / s.iloc[0]) * 100\n",
        "\n",
        "wages_b = rebase_to_100(wages_q)\n",
        "eci_b = rebase_to_100(eci_q)\n",
        "\n",
        "# 4) Plot\n",
        "plt.figure(figsize=(14, 7))\n",
        "\n",
        "plt.plot(\n",
        "    wages_b.index, wages_b.values,\n",
        "    linestyle=\"--\", color=\"red\", linewidth=2,\n",
        "    label=\"Standard Average - Biased\"\n",
        ")\n",
        "\n",
        "plt.plot(\n",
        "    eci_b.index, eci_b.values,\n",
        "    linestyle=\"-\", color=\"green\", linewidth=3.5,\n",
        "    label=\"ECI - Fixed Composition\"\n",
        ")\n",
        "\n",
        "plt.title(\"Composition Effect in Wage Measures: Standard Average vs ECI (Rebased to 2015=100)\", fontsize=15, fontweight=\"bold\")\n",
        "plt.xlabel(\"Date\")\n",
        "plt.ylabel(\"Index (2015=100)\")\n",
        "plt.legend()\n",
        "plt.grid(True, alpha=0.3)\n",
        "\n",
        "# 5) Annotate divergence in 2020 (pick the largest gap in 2020)\n",
        "gap = wages_b - eci_b\n",
        "gap_2020 = gap.loc[\"2020\"]\n",
        "\n",
        "if not gap_2020.empty:\n",
        "    spike_date = gap_2020.idxmax()\n",
        "    y_w = wages_b.loc[spike_date]\n",
        "    y_e = eci_b.loc[spike_date]\n",
        "\n",
        "    # Arrow to biased spike\n",
        "    plt.annotate(\n",
        "        \"Artificial Spike\\n(Standard Average)\",\n",
        "        xy=(spike_date, y_w),\n",
        "        xytext=(spike_date, y_w + 6),\n",
        "        arrowprops=dict(arrowstyle=\"->\", linewidth=1.5),\n",
        "        fontsize=11,\n",
        "        ha=\"center\"\n",
        "    )\n",
        "\n",
        "    # Arrow to steadier ECI\n",
        "    plt.annotate(\n",
        "        \"Stable Growth\\n(ECI: composition-fixed)\",\n",
        "        xy=(spike_date, y_e),\n",
        "        xytext=(spike_date, y_e - 8),\n",
        "        arrowprops=dict(arrowstyle=\"->\", linewidth=1.5),\n",
        "        fontsize=11,\n",
        "        ha=\"center\"\n",
        "    )\n",
        "\n",
        "plt.tight_layout()\n",
        "plt.show()\n"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/",
